<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainDashboardScreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MealTrackingMain">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/latest_meal"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="339dp" />

    <ImageButton
        android:id="@+id/MealsPageButton"
        android:layout_width="80dp"
        android:layout_height="83dp"
        android:background="@android:color/transparent"
        android:src="@drawable/group_16"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="136dp" />

    <ImageView
        android:id="@+id/FavoritesBackground"
        android:layout_width="245dp"
        android:layout_height="73dp"
        android:src="@drawable/group_12"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="239dp" />

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/HeaderCard"
        android:layout_width="412dp"
        android:layout_height="128dp"
        android:background="@color/ic_launcher_background"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageButton
        android:id="@+id/backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/arrow_back"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/profileSettingsButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="56dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/generic_avatar" />

    <ImageButton
        android:id="@+id/notificationButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="40dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/notifications" />

    <ImageView
        android:id="@+id/MealTrackingHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/meal_tracking"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="69dp" />

    <ImageView
        android:id="@+id/CreateBackground"
        android:layout_width="245dp"
        android:layout_height="73dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/frame_2"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="146dp" />

    <ImageView
        android:id="@+id/LatestMealTable"
        android:layout_width="365dp"
        android:layout_height="397dp"
        app:srcCompat="@drawable/group_14"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="328dp" />

    <ImageButton
        android:id="@+id/LogPageButton"
        android:layout_width="80dp"
        android:layout_height="83dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/group_18"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="229dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MEALS"
        android:textColor="@color/CyRed"
        tools:layout_editor_absoluteX="326dp"
        tools:layout_editor_absoluteY="210dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="27dp"
        android:layout_height="17dp"
        android:text="LOG"
        android:textColor="@color/CyRed"
        tools:layout_editor_absoluteX="344dp"
        tools:layout_editor_absoluteY="302dp" />

    <ImageButton
        android:id="@+id/AddMealButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/content"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="247dp" />

    <ImageButton
        android:id="@+id/CreateMealButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/content"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="156dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create A Meal"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add A Meal"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="253dp" />

    <ImageView
        android:id="@+id/LatestMealHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/latest_meal"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="339dp" />

</androidx.constraintlayout.widget.ConstraintLayout>